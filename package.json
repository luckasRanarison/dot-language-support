{
  "name": "dot-language-support",
  "version": "1.7.0",
  "description": "Parser and language service for graphviz (dot) files",
  "author": "Niklas Mollenhauer",
  "license": "MIT",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register tests/**/*.spec.ts tests/*.spec.ts",
    "clean": "rimraf lib",
    "compile:esm": "tsc --target esnext --module esnext --outDir lib/esm",
    "compile:cjs": "tsc --target es2017 --module commonjs --outDir lib/cjs",
    "compile": "npm run compile:esm && npm run compile:cjs",
    "prepare": "npm run clean && npm run compile"
  },
  "keywords": [
    "graphviz",
    "dot",
    "gv",
    "parser",
    "language",
    "service"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nikeee/dot-language-support.git"
  },
  "dependencies": {
    "vscode-languageserver-textdocument": "^1.0.4",
    "vscode-languageserver-types": "^3.16.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=16"
  }
}
